public class ProjetoLivro {
public static void main(String[] args) {
	Pessoa [] p = new Pessoa[2];
	Livro [] l = new Livro [3];
	
	p[0] = new Pessoa(" Marcelo " , 22 , "M");
	p[1] = new Pessoa("Maria " , 34 , "F");
	
	l[0] = new Livro("Apresentando Java " , " Luan Gomes " , 300  , p[0]);
	l[1] = new Livro ("As Cronicas de um Careca " , "Maria Eduarda"  , 500 , p[1]);
	
	

	
	System.out.println(l[0].detalhes());
	
	
}
}
